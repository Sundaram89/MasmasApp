<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android" >
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ic_pattern"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/logo1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/viga"
        android:letterSpacing="0.05"
        android:lineHeight="15.73dp"
        android:text="MasmasFood"
        android:textAlignment="center"
        android:textColor="@color/fontColor"
        android:textFontWeight="400"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/inter"
        android:lineHeight="15.73dp"
        android:text="Deliever Favorite Food"
        android:textSize="13dp"
        android:textAlignment="center"
        android:textColor="@color/fontColor1"
        android:textFontWeight="400"
        android:textColorHighlight="@color/fontColor1"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/benton"
        android:text="Sign Up For Free "
        android:textAlignment="center"
        android:textColor="@color/fontColor1"
        android:textFontWeight="400"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/name"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/buttonbackground"
        android:fontFamily="@font/benton1"
        android:drawablePadding="15dp"
        android:hint="Anamwp . . |"
        android:layout_marginEnd="20dp"
        android:paddingLeft="15dp"
        android:drawableLeft="@drawable/ic_baseline_person_24"
        android:layout_marginStart="20dp"
        android:layout_gravity="center"
        android:inputType="text"

        android:textFontWeight="400"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/email1"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_gravity="center"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/buttonbackground"
        android:drawableLeft="@drawable/ic_baseline_email_24"
        android:drawablePadding="15dp"
        android:fontFamily="@font/benton1"
        android:hint="Email"
        android:layout_marginEnd="20dp"
        android:inputType="textEmailAddress"
        android:paddingLeft="15dp"
        android:textFontWeight="400"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/password1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        app:passwordToggleTint="@color/togglecolor"
        android:background="@drawable/buttonbackground"
        android:hint="Password"
        app:startIconDrawable="@drawable/ic_lock"
        app:boxStrokeWidth="0dp"
        app:startIconTint="#15BE77"
        app:boxStrokeWidthFocused="0dp"
        app:hintAnimationEnabled="false"
        app:hintEnabled="false"
        app:layout_constraintTop_toBottomOf="@+id/email1"
        app:passwordToggleEnabled="true">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/txt_pswrd"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/buttonbackground"
            android:fontFamily="@font/inter"
            android:hint="Password"
            android:inputType="textPassword"
            android:longClickable="false"
            android:paddingVertical="10dp"
            android:paddingStart="24dp"
            android:textFontWeight="600"
            android:textSize="14sp" />

    </com.google.android.material.textfield.TextInputLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:gravity="center_vertical">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn3"
        android:layout_width="175dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:background="@drawable/button"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/benton"
        android:text="Create Account"
        android:textColor="@color/white"
        android:textAllCaps="false"
        android:textFontWeight="400"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
   />

    <TextView
        android:id="@+id/textview12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/btn3"
        android:fontFamily="@font/benton2"
        android:text="already have an account? Signup"
        android:textAlignment="center"
        android:textColor="@color/fontColor"
        android:textFontWeight="400"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn3" />
    <View
        android:layout_width="230dp"
        android:id="@+id/view"
        android:layout_height="1dp"
        android:layout_marginTop="-3dp"
        android:layout_gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview12"
        android:background="@color/fontColor"
        ></View>
    </LinearLayout>
</RelativeLayout>